<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="cacheEnabled" value="true"/>
        <!--<setting name="logImpl" value="STDOUT_LOGGING" />-->
    </settings>

    <typeHandlers>
        <typeHandler handler="com.peaceful.cron.server.modal.JobStatusTypeHandler"
                     javaType="com.peaceful.cron.server.modal.JobStatus"></typeHandler>
        <typeHandler handler="com.peaceful.cron.server.modal.JobExecuteTypeHandler"
                     javaType="com.peaceful.cron.server.modal.DispatchStatus"></typeHandler>
    </typeHandlers>
</configuration>