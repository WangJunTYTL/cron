#parse("/static/vm/public/head.vm")


<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">服务接入登记</h2>
    </div>
</div>
<div class="row animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="form-horizontal">
                <div class="form-group">
                    <label for="inputEmail3" class="col-sm-2 control-label">接入方名称</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="name" id="name"
                               placeholder="请用中文描述">
                    </div>
                </div>
                <div class="form-group">
                    <label for="inputPassword3"
                           class="col-sm-2 control-label">接入方服务标识</label>
                    <div class="col-sm-10">
                        <input type="text" class="form-control" name="cronExpression"
                               id="cronExpression" placeholder="服务唯一标识，命名请使用字母，多个单词间可以用点号连接">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-2 col-sm-10">
                        <button class="btn btn-primary" id="submitMenu">提交</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

#parse("/static/vm/public/foot.vm")
<script src="/js/cron/index.js"></script>